@startuml
!include style.puml
skinparam ArrowFontStyle plain

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":AddressBookParser" as AddressBookParser LOGIC_COLOR
participant ":NumSortCommandParser" as NumSortCommandParser LOGIC_COLOR
participant "d:NumSortCommand" as NumSortCommand LOGIC_COLOR
participant "r:CommandResult" as CommandResult LOGIC_COLOR
participant "c:AttributeBasedPersonComparator" as AttributeBasedPersonComparator LOGIC_COLOR
participant "n:NumericalValueBasedAttributeComparator" as NumericalValueBasedAttributeComparator LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant "m:Model" as Model MODEL_COLOR
end box

[-> LogicManager : execute("sort a/Graduation Year")
activate LogicManager

LogicManager -> AddressBookParser : parseCommand("sort a/Graduation Year")
activate AddressBookParser

create NumSortCommandParser
AddressBookParser -> NumSortCommandParser
activate NumSortCommandParser

NumSortCommandParser --> AddressBookParser
deactivate NumSortCommandParser

AddressBookParser -> NumSortCommandParser : parse("a/Graduation Year")
activate NumSortCommandParser

create NumSortCommand
NumSortCommandParser -> NumSortCommand
activate NumSortCommand

NumSortCommand --> NumSortCommandParser :
deactivate NumSortCommand

NumSortCommandParser --> AddressBookParser : d
deactivate NumSortCommandParser
'Hidden arrow to position the destroy marker below the end of the activation bar.
NumSortCommandParser -[hidden]-> AddressBookParser
destroy NumSortCommandParser

AddressBookParser --> LogicManager : d
deactivate AddressBookParser

LogicManager -> NumSortCommand : execute(m)
activate NumSortCommand

create NumericalValueBasedAttributeComparator
NumSortCommand -> NumericalValueBasedAttributeComparator
activate NumericalValueBasedAttributeComparator

NumericalValueBasedAttributeComparator --> NumSortCommand : n
deactivate NumericalValueBasedAttributeComparator

create AttributeBasedPersonComparator
NumSortCommand -> AttributeBasedPersonComparator: AttributeBasedPersonComparator("Graduation Year", n)
activate AttributeBasedPersonComparator

AttributeBasedPersonComparator --> NumSortCommand: c
deactivate AttributeBasedPersonComparator

NumSortCommand -> Model : sortFilteredPersonList(c)
activate Model

Model --> NumSortCommand
deactivate Model

create CommandResult
NumSortCommand -> CommandResult
activate CommandResult

CommandResult --> NumSortCommand
deactivate CommandResult

NumSortCommand --> LogicManager : r
deactivate NumSortCommand

[<--LogicManager
deactivate LogicManager
@enduml
